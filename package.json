    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@types/expect": "^24.3.0",
    "@types/mocha": "^8.2.2",
    "mocha": "^8.3.2",
    "prettier": "^2.2.1",
    "ts-mocha": "^8.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.4"
  }
